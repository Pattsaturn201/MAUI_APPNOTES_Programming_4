<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EdwinSaaNotesD.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:EdwinSaaNotesD.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="EdwinSaaNotesD">

    <TabBar>
        <ShellContent
             Title="Notes"
             ContentTemplate="{DataTemplate views:NotePage}"
             Icon="{OnPlatform 'notas.png', iOS='notas.png', MacCatalyst='notas.png'}" />
        <ShellContent
             Title="About"
             ContentTemplate="{DataTemplate views:AboutPage}"
             Icon="{OnPlatform 'informacion.png', iOS='informacion.png', MacCatalyst='informacion.png'}" />
    </TabBar>


</Shell>
